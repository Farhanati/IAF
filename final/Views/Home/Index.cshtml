@model IEnumerable<final.Models.expo>
<section id="center">
    <div class="clearfix">
        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                <li data-target="#carousel-example-generic" data-slide-to="1" class=""></li>
                <li data-target="#carousel-example-generic" data-slide-to="2" class=""></li>
            </ol>
            <div class="carousel-inner">
                <div class="item active">
                    <img src="~/MainSite/img/1.jpg" width="2000px" height="120vh">
                    <div class="carousel-caption">

                    </div>
                </div>
                <div class="item ">
                    <img src="~/MainSite/img/210.jpg" width="2000px" height="120vh">
                    <div class="carousel-caption">

                    </div>
                </div>

                <div class="item">
                    <img src="~/MainSite/img/3.jpg" width="2000px" height="120vh" alt="Third slide">
                    <div class="carousel-caption">

                    </div>
                </div>
            </div>

        </div>
        <div class="main-text hidden-xs">
            <div class="col-md-12">
                <div class="col-sm-4">
                    <div class=" center_slide">
                        <h1>INSTITUTE OF<br>FINE ARTS</h1>
                        <p>Make your dreams colorful.</p>
                        <h4> <a href="@Url.Action("Login","Register")">GET REGISTER</a></h4>
                        <h6><a href="@Url.Action("Login","Register")">CONQUER THE WORLD <i class="fa fa-play-circle"></i></a></h6>
                    </div>
                </div>
                <div class="col-sm-3"></div>
                <div class="col-sm-5">

                </div>

            </div>
        </div>
    </div>
</section>

<section id="deals_page">
    <div class="container">
        <div class="row">



            <div class="deals_page clearfix">
                @foreach (var item in ViewBag.first)
                {
                    if (item.exp_status == "Available")
                    {



                    <div class="col-sm-6">
                        <div class="deals_page_left bg_1 clearfix" style="border-radius:30px 50px 30px;">
                            <div class="col-sm-5">
                                <div class="deals_page_left_1 card-image">
                                    <a href="@Url.Action("Detail", "Home", new { @Id = item.exp_id })"><img src="~/image/@item.exp_img" width="100%" height="185px" style="border-radius:15px;"></a>
                                </div>
                            </div>
                            <div class="col-sm-7 space_left" style="border-radius:15px;">
                                <div class="deals_page_left_2">
                                    <h6>@item.exp_owner</h6>
                                    <h3><a href="@Url.Action("Detail", "Home", new { @Id = item.exp_id })">@item.exp_title</a></h3>
                                    <p>@item.exp_discription</p>
                                    <div class="clearfix deals_page_left_2_inner">
                                        <a class="tag_2" href="@Url.Action("Detail", "Home", new { @Id = item.exp_id })">Read More <i class="fa fa-arrow-right"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <p>&nbsp;</p>
                    </div>

                    }
                }
             
            </div>
        </div>
    </div>
</section>

<section id="blog_post">
    <div class="blog_post_main clearfix">
        <div class="container">
            <div class="row">
                <div class="clinic_home text-center clearfix">
                    <h1> LATEST BLOGS</h1>
                    <p>  Sed augue semper porta.  Vestibulum lacinia arcu eget nulla.Class aptent taciti sociosqu ad litora torquent per<br> conubia nostra, per inceptos himenaeos Sed dignissim lacinia nunc.</p>
                </div>
                <div class="blog_post clearfix">
                    @{
                        foreach (var item in ViewBag.data)
                        {
                            if (item.exp_status == "Available")
                            {


                                <div class="col-sm-4">
                                    <div class="blog_post_inner" style="background:#ffff;">
                                        <div class="card-image"><img src="~/image/@item.exp_img" width="100%" height="300px"></div>
                                        <h5 class="text-center">@item.exp_date -January - 2022</h5>
                                        <h4 class="text-center"><a href="@Url.Action("Detail", "Home", new { @Id = item.exp_id })">@item.exp_title</a></h4>
                                        <p class="text-center">@item.exp_discription</p>
                                    </div>
                                    <p>&nbsp;</p>
                                </div>

                            }
                        }
                    }
                </div>
                <div class="services_home_2 text-center clearfix">
                    <h5><a href="@Url.Action("collection","Home")">View Our Blogs</a></h5>
                </div>
            </div>
        </div>
    </div>
</section>

<section id="team_home">
